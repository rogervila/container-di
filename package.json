{
    "name": "container-di",
    "version": "2.0.0",
    "description": "A lightweight Dependency Injection (DI) container library for TypeScript with singleton support",
    "author": "Roger Vil√†",
    "license": "MIT",
    "type": "module",
    "main": "dist/container-di.umd.cjs",
    "module": "dist/container-di.js",
    "exports": {
        ".": {
            "types": "./dist/container-di.d.ts",
            "import": "./dist/container-di.js",
            "require": "./dist/container-di.umd.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "vite",
        "build": "tsc && vite build",
        "test": "vitest run --dom",
        "format": "npx @biomejs/biome check --write",
        "lint": "tsc && npx @biomejs/biome ci ."
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.8",
        "happy-dom": "^20.0.11",
        "typescript": "~5.9.3",
        "vite": "^7.2.7",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^4.0.15"
    }
}
